<!DOCTYPE html>

<html class="no-js">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>{% block title %}AUC{% endblock %}</title>
    <link rel="shortcut icon" href="images/logo.png">
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro" rel="stylesheet">
    <!-- Animate.css -->
    <link rel="stylesheet" href="{{ asset('css/animate.css') }}">
    <!-- Icomoon Icon Fonts-->
    <link rel="stylesheet" href="{{ asset('css/icomoon.css') }}">
    <!-- Bootstrap  -->
    <link rel="stylesheet" href="{{ asset('css/bootstrap.css') }}">
    <!-- Superfish -->
    <link rel="stylesheet" href="{{ asset('css/superfish.css') }}">
    <!-- Magnific Popup -->
    <link rel="stylesheet" href="{{ asset('css/magnific-popup.css') }}">
    <!-- Date Picker -->
    <link rel="stylesheet" href="{{ asset('css/bootstrap-datepicker.min.css') }}">
    <!-- CS Select -->
    <link rel="stylesheet" href="{{ asset('css/select.css') }}">
    <link rel="stylesheet" href="{{ asset('css/animate.css') }}">

    <link rel="stylesheet" href="{{ asset('css/style.css') }}">


    <!-- Modernizr JS -->
    <script src="{{ asset('js/modernizr-2.6.2.min.js') }}"></script>

    <!-- Header Menú -->
</head>
<body>
    <div id="fh5co-wrapper">
        <div id="fh5co-page">

            <header id="fh5co-header-section" class="sticky-banner">
            <div class="container">
                <div class="nav-header">
                    <a href="#" class="js-fh5co-nav-toggle fh5co-nav-toggle dark"><i></i></a>
                    <h1 id="fh5co-logo"><a href="{{ asset('/') }}"><img src="/images/logo.png" class="logo"></i>Athletic Unión Catarroja </a></h1>
                    <!-- Empieza el menú -->
                    <nav id="fh5co-menu-wrap" role="navigation">
                        <ul class="sf-menu" id="fh5co-primary-menu">
                            <li>
                                <a href="#" class="fh5co-sub-ddown">Fútbol</a>
                                <ul class="fh5co-sub-menu">
                                    <li><a href="{{ asset('futbol_convencional') }}">Fútbol convencional</a></li>
                                    <li><a href="{{ asset('futbol_adaptado') }}">Futbol adaptado</a></li>
                                    <li><a href="{{ asset('futbol_silla_ruedas') }}">Futbol silla de ruedas</a></li>
                                </ul>
                            </li>
                            <li>
                                <a href="{{ asset('actividades_extra') }}" >Actividades extra</a>
                            </li>
                            <li><a href="{{ asset('nosotros') }}">Quienes somos</a></li>
                            <li><a href="{{ asset('contacto') }}">Contacto</a></li>
                        </ul>
                    </nav>
                </div>
            </div>
        </header>
            <!-- Final del header -->
            {% block body %}{% endblock %}
        <footer>
            <div id="footer">
                <div class="container">
                    <div class="row">
                        <div class="col-md-6 col-md-offset-3 text-center">
                            <p class="fh5co-social-icons">
                                <a href="https://es-es.facebook.com/aucatarroja/"><i class="icon-facebook2"></i></a>
                                <a href="https://twitter.com/aucatarroja?lang=es"><i class="icon-twitter2"></i></a>
                            </p>
                            <p>Copyright 2017 <a href="www.floridauniversitaria.es">Florida Universitaria</a>. Todos los derechos reservados. <br>Powered by Florida Universitaria</a>
                            <p><a href="{{ asset('/admin') }}"><img id="logiin" src="{{ asset('images/login.svg')}}"></a>
                            {% if is_granted('ROLE_ADMIN') %}
                            <a href="{{ asset('/admin/logout') }}"><img id="logout" src="{{ asset('images/logout.svg')}}"></a></p>
                            {% endif %}
                            </div>
                        </div>
                    </div>
                </div>
            </footer>



        </div>
        <!-- END fh5co-page -->

    </div>
    <!-- END fh5co-wrapper -->

    <!-- jQuery -->

    <script src="{{ asset('js/jquery.min.js') }}"></script>
    <!-- jQuery Easing -->
    <script src="{{ asset('js/jquery-easing.1.3.js') }}"></script>
    <!-- Bootstrap -->
    <script src="{{ asset('js/bootstrap.min.js') }}"></script>
    <!-- Waypoints -->
    <script src="js/jquery.waypoints.min.js"></script>
    <script src="js/sticky.js"></script>

    <!-- Stellar -->
    <script src="js/jquery.stellar.min.js"></script>
    <!-- Superfish -->
    <script src="js/hoverIntent.js"></script>
    <script src="js/superfish.js"></script>
    <!-- Magnific Popup -->
    <script src="js/jquery.magnific-popup.min.js"></script>
    <script src="js/magnific-popup-options.js"></script>
    <!-- Date Picker -->
    <script src="js/bootstrap-datepicker.min.js"></script>
    <!-- CS Select -->
    <script src="js/classie.js"></script>
    <script src="js/selectFx.js"></script>

    <!-- Main JS -->
    <script src="js/main.js"></script>


</body>
</html>
